<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FormInputTextComponent">
        <attr name="titleText" format="string" localization="suggested" />
        <attr name="hintDescription" format="string" localization="suggested" />
        <attr name="optionalField" format="boolean" />
        <attr name="inputTypeNumber" format="integer" />
    </declare-styleable>

    <declare-styleable name="CheckboxSelectionComponent">
        <attr name="checkGroupText" format="string" localization="suggested" />
    </declare-styleable>

    <declare-styleable name="ButtonComponent">
        <attr name="title" format="string" localization="suggested" />
    </declare-styleable>

    <declare-styleable name="SimpleActionChoiceComponent">
        <attr name="actionTitle" format="string" localization="suggested" />
    </declare-styleable>

    <declare-styleable name="SimpleToolbarComponent">
        <attr name="toolbarTitle" format="string" localization="suggested" />
    </declare-styleable>

    <declare-styleable name="SimpleFilterComponent">
        <attr name="filterWidth" format="float" />
        <attr name="filterHeight" format="float" />
        <attr name="filterMarginStart" format="float" />
    </declare-styleable>

    <declare-styleable name="QuizResultItemComponent">
        <attr name="resultLabel" format="string" />
        <attr name="includeTopSeparator" format="boolean" />
    </declare-styleable>

</resources>