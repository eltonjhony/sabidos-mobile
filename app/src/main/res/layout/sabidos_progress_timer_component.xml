<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="198dp"
    android:layout_height="198dp"
    android:background="@color/colorPrimary">

    <com.sabidos.presentation.components.CircularSeekBar
        android:id="@+id/circularSeekComponent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:cs_negative_enabled="false"
        app:cs_circle_style="round"
        app:cs_max="168"
        app:cs_progress="0"
        app:cs_circle_fill="@android:color/transparent"
        app:cs_circle_color="@color/disableButtonColor"
        app:cs_circle_progress_color="@color/colorWhite"
        app:cs_pointer_color="@color/colorWhite"
        app:cs_circle_stroke_width="8dp"
        app:cs_pointer_stroke_width="18dp"
        app:cs_pointer_icon="@mipmap/ic_launcher_round"
        app:cs_disable_pointer="false"
        app:cs_pointer_angle="1" />

</androidx.constraintlayout.widget.ConstraintLayout>